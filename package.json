{
  "name": "archmetrics-studio",
  "version": "1.0.0",
  "description": "Archmetrics Studio's website",
  "scripts": {
    "start": "cd client && npm run build && cd ../server && npm start",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\"",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build-client": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdullahalshawafi/Archmetrics-Studio.git"
  },
  "keywords": [
    "nodejs",
    "website",
    "mongodb",
    "reactjs",
    "expressjs",
    "mern-stack"
  ],
  "authors": [
    "Abdullah Adel",
    "Mohamed Fathy"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/abdullahalshawafi/Archmetrics-Studio/issues"
  },
  "homepage": "https://github.com/abdullahalshawafi/Archmetrics-Studio#readme",
  "devDependencies": {
    "concurrently": "^6.3.0"
  }
}
